@model IRS.Web.ViewModels.User.UserListViewModel
@using YuSpin.Fw.Web

@{
    ViewBag.Title = "Top 5";
}

<div data-bind="with: top5.viewModel">   
    <table id="top5Table" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
        </thead>
    </table>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/top5js")
    <script type="text/javascript">
        @{
            var data = JsonHelper.Convert(Model);
            @Html.Raw($"top5.init('{data}');");
        }
    </script>
}